[Unit]
Description=Kubernetes Kube Proxy
Documentation=https://github.com/kubernetes/kubernetes

[Service]
ExecStart={{ kube_proxy_binary_local_path }}/{{ kube_proxy_binary_name }} \
  --config={{ kube_proxy_work_dir }}/kube-proxy-config.yaml
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
